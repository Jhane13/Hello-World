//program using conditional Statement.
#include<stdio.h>
 int main() {
 
    int age,name;
    printf("Name:Juan Dela Cruz\nAge:");
    scanf("%d",&name);
    
    if(age<17){
     printf("You are not eligible for voting!\n");
    }
    printf("Name: Juan Delema\nAge:");
    scanf("%d",&name);
     if(age<=18){
    printf("You are eligible for voting!");
    }
    printf("\nName:Juan Delama DeGuzman\nAge:");
    scanf("%d",&name);
    if(age!=18){
      printf("Invalid Age, Please Try Again!");
    }
    return 0;
}



    